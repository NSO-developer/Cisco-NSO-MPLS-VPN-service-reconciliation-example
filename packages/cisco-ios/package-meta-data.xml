<?xml version="1.0"?>
<ncs-package xmlns="http://tail-f.com/ns/ncs-packages">
  <name>cisco-ios</name>
  <package-version>3.8.0</package-version>
  <description>NED package for Cisco IOS</description>
  <ncs-min-version>3.2.2</ncs-min-version>
  <ncs-min-version>3.3</ncs-min-version>
  <ncs-min-version>3.4</ncs-min-version>
  <component>
    <name>cisco-ios</name>
    <ned>
      <cli>
        <ned-id xmlns:ios-id="urn:ios-id">ios-id:cisco-ios</ned-id>
        <java-class-name>com.tailf.packages.ned.ios.IOSNedCli</java-class-name>
      </cli>
      <device>
        <vendor>Cisco</vendor>
      </device>
      <option>
        <name>show-tag</name>
        <value>interface</value>
      </option>
    </ned>
  </component>
  <component>
    <name>IOSDp</name>
    <callback>
      <java-class-name>com.tailf.packages.ned.ios.IOSDp</java-class-name>
    </callback>
  </component>
  <component>
    <name>IOSDp2</name>
    <callback>
      <java-class-name>com.tailf.packages.ned.ios.IOSDp2</java-class-name>
    </callback>
  </component>
